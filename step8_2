N = int(input())

first = 1

plus = 6

room = 1

if N == 1:

    print(1)

else:

    while True:

        first = first + plus

        room+= 1

        if N <= first:

            print(room)

            break

        plus += 6
        
        
        
        
        
        
x = int(input())
 
count = 0
 
while x > 0:
    x -= count
    count += 1
 
x = count + x - 1
 
anw = str(x) + '/' + str(count - x)
if count % 2 == 0:
    anw = str(count - x) + '/' + str(x)
print(anw)
