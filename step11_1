def fac(n):
    if n<=1: 
        return 1
    else: 
        return n * fac(n-1)

print(fac(int(input())))


e = [0, 1]

def fib(n):
    if 0 <= n < 21:
        for i in range(2, n + 1):
            e.append(e[i - 1] + e[i - 2])
        return print(e[n])
    else:
        return print('1 이상 20 이하의 자연수나 0을 입력하세요')
    
fib(int(input()))
